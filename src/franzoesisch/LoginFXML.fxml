<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="272.0" prefWidth="400.0" styleClass="pane-login" stylesheets="@stylesheet.css" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="franzoesisch.LoginFXMLController">
   <children>
      <Label alignment="CENTER" layoutX="103.0" layoutY="49.0" prefHeight="30.0" prefWidth="248.0" text="SIGN IN" AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <TextField fx:id="mail" layoutX="102.0" layoutY="97.0" prefHeight="30.0" prefWidth="200.0" promptText="Email:" AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0" />
      <TextField fx:id="password" layoutX="100.0" layoutY="137.0" prefHeight="30.0" prefWidth="200.0" promptText="Password:" AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0" />
      <Button layoutX="100.0" layoutY="179.0" mnemonicParsing="false" onAction="#submit" prefHeight="30.0" prefWidth="200.0" styleClass="button-login" text="Submit" AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0" />
      <HBox layoutX="-1.0" layoutY="-1.0" prefHeight="50.0" prefWidth="300.0">
         <children>
            <Label alignment="CENTER" onMouseClicked="#register" prefHeight="26.0" prefWidth="78.0" text="Register">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
         </children>
      </HBox>
   </children>
</AnchorPane>
